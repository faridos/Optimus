        <script src="{{ asset('template/js/calendar.js') }}"></script>
        <script src="{{ asset('template/js/selectinput.js') }}" type=template/text/javascript" ></script>
        <script src="{{ asset('template/js/write.js') }}" type="text/javascript"></script>
