<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
        <title>{% block title %}Optimus{% endblock %}</title>

        <link href="{{ asset('bundles/dcsrating/css/rating.css') }}" type="text/css" rel="stylesheet" />

        <link href="{{ asset('template/css/font-awesome.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/simple-line-icons.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/fullscreen-slider.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/app.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/form.css') }}" type="text/css" rel="stylesheet" />
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('template/css/datepicker.css') }}" rel="stylesheet">
        {% block stylesheets %}

        {% endblock %}

    </head>
    <body class="notransition">


        <!-- Hero -->
        {% block navigation %}
        {% endblock %}


        {% block body %}
        {% endblock %}

        {% block footer %}
        {% endblock %}

        <script src="{{ asset('bundles/dcsrating/js/rating.js') }}"></script>

        <script src="{{ asset('template/js/json2.js') }}"></script>
        <script src="{{ asset('template/js/jquery-2.1.1.min.js') }}"></script>
        <script src="{{ asset('template/js/underscore.js') }}"></script>
        <script src="{{ asset('template/js/moment-2.5.1.js') }}"></script>
        <script src="{{ asset('template/js/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('template/js/jquery-ui-touch-punch.js') }}"></script>
        <script src="{{ asset('template/js/jquery.placeholder.js') }}"></script>
        <script src="{{ asset('template/js/bootstrap.js') }}"></script>
        <script src="{{ asset('template/js/jquery.touchSwipe.min.js') }}"></script>
        <script src="{{ asset('template/js/jquery.slimscroll.min.js') }}"></script>
        <script src="{{ asset('template/js/jquery.visible.js') }}"></script>
        <script src="http://maps.googleapis.com/maps/api/js?sensor=true&amp;libraries=geometry&amp;libraries=places" type="text/javascript"></script>
        <script src="{{ asset('template/js/infobox.js') }}"></script>
        <script src="{{ asset('template/js/clndr.js') }}"></script>
        <script src="{{ asset('template/js/jquery.tagsinput.min.js') }}"></script>
        <script src="{{ asset('template/js/bootstrap-datepicker.js') }}"></script>
        <script src="{{ asset('template/js/app.js') }}"></script>
        <script src="{{ asset('template/js/calendar.js') }}"></script>

        {% block javascripts %}
        {% endblock %}
    </body>
</html>
