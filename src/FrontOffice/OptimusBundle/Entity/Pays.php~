<?php

namespace FrontOffice\OptimusBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * Pays
 * @ORM\Entity(repositoryClass="FrontOffice\OptimusBundle\Repository\PaysRepository")
 * @ORM\HasLifecycleCallbacks
 * @ORM\Table(name="pays")
 */
class Pays {
     /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    /**
     * @var string
     *
     * @ORM\Column(name="nom", type="string", length=255)
     */
    private $nom;
     /**
    * @ORM\OneToMany(targetEntity="Region", mappedBy="pays", cascade={"persist","remove"})
    **/
    protected $regions;
     public function __construct()
    {
        
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nom
     *
     * @param string $nom
     * @return Pays
     */
    public function setNom($nom)
    {
        $this->nom = $nom;
    
        return $this;
    }

    /**
     * Get nom
     *
     * @return string 
     */
    public function getNom()
    {
        return $this->nom;
    }

    /**
     * Add regions
     *
     * @param \FrontOffice\OptimusBundle\Entity\Region $regions
     * @return Pays
     */
    public function addRegion(\FrontOffice\OptimusBundle\Entity\Region $regions)
    {
        $this->regions[] = $regions;
    
        return $this;
    }

    /**
     * Remove regions
     *
     * @param \FrontOffice\OptimusBundle\Entity\Region $regions
     */
    public function removeRegion(\FrontOffice\OptimusBundle\Entity\Region $regions)
    {
        $this->regions->removeElement($regions);
    }

    /**
     * Get regions
     *
     * @return \Doctrine\Common\Collections\Collection 
     */
    public function getRegions()
    {
        return $this->regions;
    }
}